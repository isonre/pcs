<%@ page pageEncoding="UTF-8" contentType="text/html;charset=UTF-8"%>
<%@ include file="/WEB-INF/jsp/common/taglibs.jsp"%>
<html>
	<head>
		<link rel="stylesheet" type="text/css" media="screen" href="${ctx}/assets/bootstrap/css/bootstrap.min.css" />
		<script>
			function closewin() {
				window.close();
			}
		</script>
	</head>
	<body align="center">
		<table width="99%" height="93%" align="center">
			<tr>
				<td align="right" height="50px">
					<button type="button" onclick="closewin()" class="btn btn-lg btn-danger">关闭</button>
				</td>
			</tr>
			<tr>
				<td>
					<iframe src="${url}" width="100%" height="100%" frameborder="no"></iframe>
				</td>
			</tr>
		</table>
		<%/*
		<script>
		window.location.href = '${url}';
		</script>
		*/%>
	</body>
</html>