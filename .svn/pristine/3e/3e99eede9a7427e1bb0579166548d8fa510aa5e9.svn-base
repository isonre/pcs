<?xml version="1.0" encoding="UTF-8"?>
  <modeltable id="testTable" width="100%" height="100%" tableName="B_ANALYSIS" dataSource="oracle">
      <view><![CDATA[
        select rownum,
          c.*
          from (
          select
            distinct
            b.crop_name,
            avg(b.price) over(partition by to_char(b.time, 'YYYY-MM'),b.crop_name) as avgPrice,
            pre.avgPrice as preAvgPrice,
            (avg(b.price) over(partition by to_char(b.time, 'YYYY-MM'),b.crop_name) - pre.avgPrice)/pre.avgPrice as changeRandge,                
            b.unit,
            b.market,
            b.information,
            to_char(b.time, 'YYYY-MM') as tt
         from B_ANALYSIS b,(
            select
              crop_name,  
              to_char(time, 'YYYY-MM') as tt,
              avg(price) as avgPrice
            from B_ANALYSIS
            where  
              market = '#{market}'
            group by
              crop_name,
              to_char(time, 'YYYY-MM')
          ) pre
          where 
            market = '#{market}' and
            b.crop_type = '#{ncplb}' and
            b.crop_name = pre.crop_name and
            to_char(add_months(b.time, -1), 'YYYY-MM') = pre.tt
          ) c
        where c.tt = '#{cdate}'
      ]]>
      </view>
      <target name="rownum" dataType="num" sort="true" label="序号">rownum</target>
      <target name="market" dataType="str" label="交易地">market</target>
      <target name="cropName" dataType="str" sort="true" label="名称">crop_name</target>
      <target name="avgPrice" dataType="num" label="本月平均价格" format=".##">avgPrice</target>
      <target name="preAvgPrice" dataType="num" label="上月平均价格" format=".##">preAvgPrice</target>
      <target name="changeRandge" dataType="num" label="涨跌幅度" format="##%">changeRandge</target>
      <target name="unit" dataType="str" label="计量单位">unit</target>
      <target name="information" dataType="num" label="信息来源">information</target>
      <target name="time" dataType="str" label="时间">tt</target>
  </modeltable>